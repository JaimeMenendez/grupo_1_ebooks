    <ul class="subcategory-books-pc">
        <% subcategorias.forEach(subcategoria => {%>
            <li><a id="subcategory" href=""><%=subcategoria%></a></li>
        <% }) %>
    </ul>
<% } else{ %>
    <ul class="subcategory-books-pc">
        <% subcategorias.forEach(subcategoria => {%>
            <li><a id="subcategory" href=""><%=subcategoria%></a></li>
        <% }) %>
    </ul>
<% } %>